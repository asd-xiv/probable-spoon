{
  "name": "tpl-react",
  "version": "0.0.1",
  "description": "Frontend starter with React, Redux, Parcel and friends",
  "license": "MIT",
  "homepage": "https://github.com/andreidmt/tpl-react#readme",
  "repository": {
    "type": "git",
